# Development Server Configuration
DEV_PORT=3000
NODE_ENV=development

# Local Firebase Configuration (for development only)
# These variables use LOCAL_FB_ prefix to avoid conflicts with Cloud Functions reserved prefixes
LOCAL_FB_PROJECT_ID=your-project-id
LOCAL_FB_CLIENT_EMAIL=your-client-email@your-project-id.iam.gserviceaccount.com
LOCAL_FB_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"

# CORS
CORS_ORIGIN=http://localhost:4200
